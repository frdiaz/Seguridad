<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="Seguridad.Models" assembly="Seguridad">
  <class name="super_administrador" table="super_administrador" lazy="false">
    <id name="id_super_administrador" column="id_super_administrador">
      <generator class="identity" />
    </id>
    <property name="username" column="username" not-null="true" />
    <property name="password" column="password" not-null="true" />
    <property name="nombre" column="nombre" not-null="true" />
    <property name="apellido" column="apellido" not-null="true" />
    <property name="estado" column="estado" not-null="true" />
  </class>
</hibernate-mapping>