<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="Seguridad.Models" assembly="Seguridad">
  <class name="UsuarioEmpresaPerfil" table="usuario_empresa_perfil" lazy="false">
    <id name="id_usuario_empresa" column="id_usuario_empresa">
      <generator class="identity" />
    </id>
    <property name="rut_empresa" column="rut_empresa" not-null="true" />
    <property name="rut_usuario" column="rut_usuario" not-null="true" />
    <property name="id_perfil" column="id_perfil" not-null="true" />
    <property name="estado" column="estado" not-null="true" />
  </class>
</hibernate-mapping>